<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wzp.mouth.eat.mapper.LotteryMapper">

    <insert id="insert">
        insert into lottery(id,date,number) values (#{id},#{date},#{number})
    </insert>

    <select id="select"  resultType="com.wzp.mouth.eat.dao.Lottery">
        select * from lottery
    </select>

</mapper>